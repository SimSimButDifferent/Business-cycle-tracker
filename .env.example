# Business Cycle Tracker API Keys
# Copy this file to .env and replace with your actual keys

# FRED API key from https://fred.stlouisfed.org/docs/api/api_key.html
FRED_API_KEY=your_fred_api_key_here

# Alpha Vantage API key (optional, for additional data sources)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# Coinbase API key (optional, for alternative crypto data)
COINBASE_API_KEY=your_coinbase_key_here
COINBASE_API_SECRET=your_coinbase_secret_here 